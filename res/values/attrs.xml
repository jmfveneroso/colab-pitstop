<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="CustTextView">
    <!-- ** This enum is tightly coupled with the enum in FontManager! ** -->
    <!-- ** Make sure their orders stay the same ** -->
    <attr name="cust_font" >
      <enum name="SourceSansPro_Black" value="0"/>
      <enum name="SourceSansPro_BlackItalic" value="1"/>
      <enum name="SourceSansPro_Bold" value="2"/>
      <enum name="SourceSansPro_BoldItalic" value="3"/>
      <enum name="SourceSansPro_ExtraLight" value="4"/>
      <enum name="SourceSansPro_ExtraLightItalic" value="5"/>
      <enum name="SourceSansPro_Italic" value="6"/>
      <enum name="SourceSansPro_Light" value="7"/>
      <enum name="SourceSansPro_LightItalic" value="8"/>
      <enum name="SourceSansPro_Regular" value="9"/>
      <enum name="SourceSansPro_Semibold" value="10"/>
      <enum name="SourceSansPro_SemiboldItalic" value="11"/>
    </attr>
  </declare-styleable>
  <declare-styleable name="PriceDisplayer">
    <attr name="dialog_title" format="string" />
    <attr name="digits" format="integer" />
    <attr name="editable" format="boolean" />
    <attr name="pattern" format="string" />
    <attr name="changecolor" format="boolean" />
  </declare-styleable>
  <declare-styleable name="StarGroup">
    <attr name="star_editable" format="boolean" />
    <attr name="star_size" format="string" />
  </declare-styleable>
  <declare-styleable name="SeekBarPreference">
      <attr name="minValue" format="integer" />
      <attr name="maxValue" format="integer" />
  </declare-styleable>


</resources>